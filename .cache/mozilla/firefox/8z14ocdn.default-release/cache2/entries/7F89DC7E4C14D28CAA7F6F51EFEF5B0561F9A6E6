      var typed_library_script = document.getElementById('typed-script')
      if ((document.getElementById('typed-script') === null)) {
        var script = document.createElement('script')
        script.setAttribute('type', 'text/javascript')
        script.setAttribute('id', 'typed-script')
        script.setAttribute('src', 'https://cdn.jsdelivr.net/npm/typed.js@2.0.11')
        document.currentScript.parentNode.appendChild(script)
        script.onload = function () {
          var event = new CustomEvent('execute')
          document.dispatchEvent(event)
        }
      }
    </script>
    <script>
      document.addEventListener('execute', (function (currentScript) {
        return function () {
          var parent = currentScript.parentNode
          var original_list = JSON.parse(parent.getAttribute('data-text-list').replace(/'/g, '"'))
          var original_text = parent.querySelector('.animated-text').textContent
          parent.querySelector('.animated-text').innerHTML = ""
          var text_list = []
          for (var i = 0; i < original_list.length; i++) {
            text_list[i] = '^1000' + original_list[i].animatable_text + '^1500'
          }
          text_list.unshift(original_text)
          var options = {
            strings: text_list,
            stringsElement: null,
            typeSpeed: 70,
            backSpeed: 70,
            loop: true,
            loopCount: Infinity,
            smartBackspace: true
          };

          var typed = new Typed(parent.querySelector('.animated-text'), options);
        }
      })(document.currentScript))
    </script>
