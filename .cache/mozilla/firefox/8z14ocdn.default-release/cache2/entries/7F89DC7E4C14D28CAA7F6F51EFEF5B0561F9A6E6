<script type="text/javascript">
  FW.onLoad(function () {
    const $featureContent = FW.$('#feature-content-73728205816542');
    const $featureHeading =  $featureContent.find('.feature-heading');
    const heading = $featureHeading.length > 1 ? $featureHeading[1].textContent : $featureHeading[0].textContent;
